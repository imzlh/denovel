{
  "tasks": {
    "server": "deno run -A --unstable-raw-imports --unstable-ffi --unstable-kv server.ts",
    "epub": "deno run -A --unstable-raw-imports --unstable-ffi --unstable-kv 2epub.ts",
    "dev": "deno run -A --watch --unstable-raw-imports --unstable-ffi --unstable-kv server.ts",
    "down": "deno run -A --unstable-raw-imports --unstable-ffi main.ts -e -s 2 -w",
    "lanzou": "deno run -A --unstable-raw-imports lanzoudl.ts",
    "comic": "deno run -A --unstable-raw-imports --unstable-ffi comic.ts",
    "build": "deno compile --unstable-raw-imports --unstable-kv --allow-all --include=data.d.ts --include templates --include static  --output=app.exe exe.ts",
    "fem3u8": "deno run -A specialm3u8.ts"
  },
  "imports": {},
  "nodeModulesDir": "auto",
  "license": "MIT",
  "version": "1.8.2"
}
